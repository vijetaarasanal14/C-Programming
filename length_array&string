#include <stdio.h>

int main() {
    int a[] = {10, 20, 30, 40, 50};

    int length = sizeof(a) / sizeof(a[0]);

    printf("Length of integer array = %d\n", length);

    return 0;
}

#include <stdio.h>
#include <string.h>

int main() {
    char name[] = "Vijeta";
    int len = strlen(name);
    printf("Length = %d\n", len);
    return 0;
}
